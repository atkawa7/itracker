<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping auto-import="true">

	<class name="org.itracker.model.IssueField" table="issuefieldbean">
		<id name="id" column="id" type="int" unsaved-value="null">
			<generator class="native"/>
		</id>
		<property name="stringValue" column="string_value" not-null="true" unique="false"/>		
		<property name="intValue" column="int_value" not-null="true" unique="false"/>				
		<property name="dateValue" column="date_value" not-null="true" unique="false"/>		
		<many-to-one name="issue" column="issue_id" class="org.itracker.model.Issue"/>		
		<many-to-one name="customField" column="field_id" class="org.itracker.model.CustomField"/>		
	</class>

</hibernate-mapping>
